<div style="width: 100%; overflow: hidden;">
    <div style="width:80%; margin: 0 auto; float:left;">
      <div class="container">

      <h2>Which hood is the greenest?</h2>

      <% ranked = @hoods.sort_by { |hood| hood.hood_score }.reverse %>
      <ol>
        <% ranked.each do |hood| %>
          <% if hood == ranked.first %>
            <h2><li class="hood-list"><%= link_to hood.name, hood %> - <%= hood.hood_score %></li></h2>
          <% else %>
            <li class="hood-list"><%= link_to hood.name, hood %> - <%= hood.hood_score %></li>
          <% end %>
        <% end %>
      </ol>

      </div>
    </div>

    <div style="width: 55%; margin-left: auto; float: right;">
      <img src="seattle-hoods-map.jpg" height="500px"/>
    </div>

</div>

